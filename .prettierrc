{
  "printWidth": 100,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": false,
  "jsxBracketSameLine": false,
  "parser": "babylon",
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "tabWidth": 2,
        "parser": "css",
        "printWidth": 80,
      }
    }
  ]
}
